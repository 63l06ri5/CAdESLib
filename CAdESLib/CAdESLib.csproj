  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="LdapForNet" Version="2.7.10" />
    <PackageReference Include="NLog" Version="4.7.0" />
    <PackageReference Include="Portable.BouncyCastle-GOST2012" Version="1.8.6.11" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
