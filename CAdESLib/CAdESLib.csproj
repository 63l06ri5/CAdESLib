<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Ilya Turkin</Authors>
    <Company />
    <Description>This is rework of https://github.com/nonorganic/dssnet</Description>
    <PackageProjectUrl>https://github.com/63l06ri5/CAdESLib</PackageProjectUrl>
    <RepositoryUrl>https://github.com/63l06ri5/CAdESLib</RepositoryUrl>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <Version>1.6.3.1</Version>
